{
  "ssg": "eleventy",
  "mode": "hosted",
  "build": {
    "preserved_paths": "node_modules/,.cache/,dist/assets/images/",
    "environment_variables": [
      {
        "key": "CLOUDCANNON_ENV",
        "value": "cloudcannon"
      }
    ],
    "node_version":"file",
    "output_path": "dist",
    "install_command": "npm i",
    "build_command": "npm run eleventy"
  }
}